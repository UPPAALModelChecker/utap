<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.6//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_6.dtd'>
<nta>
	<declaration>// Place global declarations here.
broadcast chan A, B;

chan priority default &lt; A;</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<declaration>// Place local declarations here.
</declaration>
		<location id="id0" x="-102" y="0">
			<name x="-112" y="-34">Goal</name>
		</location>
		<location id="id1" x="110" y="0">
			<name x="100" y="-34">Error</name>
		</location>
		<location id="id2" x="0" y="0">
			<committed/>
		</location>
		<init ref="id2"/>
		<transition id="id3">
			<source ref="id2"/>
			<target ref="id0"/>
			<label kind="synchronisation" x="-59" y="-25">A!</label>
		</transition>
		<transition id="id4">
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="42" y="-25">B!</label>
		</transition>
	</template>
	<system>system Template;
</system>
	<queries>
		<query>
			<formula>A&lt;&gt; Process.Goal</formula>
			<comment/>
		</query>
	</queries>
</nta>
